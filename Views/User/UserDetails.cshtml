@model  UserDetailsViewModel
@{
    ViewBag.Title = "User Details";
}

<h3>@Html.DisplayFor(model => model.PageTitle)</h3>
<hr />
<div>
 
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.User.UserID)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.User.UserID)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.User.UserName)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.User.UserName)
        </dd>

        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.User.UserPass)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.User.UserPass)
        </dd>

        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.User.RoleId)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.User.RoleId)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.User.UserEmail)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.User.UserEmail)
        </dd>

    </dl>
</div>

@section Scripts{ 
    <script src="~/js/CustomScript.js"></script>
}
